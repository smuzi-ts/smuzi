{
"compilerOptions": {
    "target": "ES2024",
    "module": "nodenext",
    "lib": ["ESNext", "dom"],
    "allowJs": false,
    "checkJs": false,
    "noEmit": true,
    "strict": false,
    "strictNullChecks": true,
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": "."
  }
}