{
"compilerOptions": {
    "target": "ES2024",
    "module": "ESNext",
    "lib": ["ESNext", "dom"],
    "allowJs": false,
    "checkJs": false,
    "noEmit": true,
    "strict": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "baseUrl": ".",
    "paths": {
      "#std": ["./jis/std/src/*"]
    }
  }
}